public interface IViewHolderOperate<T> {

    void reset(@NonNull RecyclerView.ViewHolder holder);

    void showPre();

    void showNext(T item);

    void notifyFocusChanged(int position);

    void notifyDataSet();

    boolean dispatchAction(@NonNull String intent,int value);
}
