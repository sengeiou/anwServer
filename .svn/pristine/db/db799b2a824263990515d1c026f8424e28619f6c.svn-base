<?xml version="1.0" encoding="utf-8"?>
<com.lunzn.tool.autofit.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.inovel.setting.ui.Set3DModelActivity"
    >

    <com.lunzn.tool.autofit.AutoRadioGroup
        android:id="@+id/rg_3d_model"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center">
        <com.lunzn.tool.autofit.AutoRadioButton
            android:id="@+id/left_right_model"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="打开左右3D模式"/>
        <com.lunzn.tool.autofit.AutoRadioButton
            android:id="@+id/up_down_model"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="打开上下3D模式"/>
        <com.lunzn.tool.autofit.AutoRadioButton
            android:id="@+id/close_model"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关闭3D模式"/>
    </com.lunzn.tool.autofit.AutoRadioGroup>

</com.lunzn.tool.autofit.AutoRelativeLayout>